<!-- Running a set of test cases together is call executing a Test Suite. -->
<!-- In TestNG framework, -->
<!-- we need to create testng.xml file to create and handle multiple test
classes. -->
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Test-credentials">
    <test name="test-login">

        <parameter name="username1" value="testWrongUser" />
        <parameter name="password1" value="TestWrongPass" />
        <parameter name="username2" value="everton@gmail.com" />
        <parameter name="password2" value="123abc" />

        <classes>

            <class name="testNG.Assignment_final_exam2" />
            
        </classes>

    </test>
</suite>