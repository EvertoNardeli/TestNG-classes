<!-- Running a set of test cases together is call executing a Test Suite. -->
<!-- In TestNG framework, -->
<!-- we need to create testng.xml file to create and handle multiple test
    classes. -->
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Suite" >  
 <test name="Method Test Cases" >
    <classes>
       <class name="testNG.dataProvider">
        <methods>
        <include name="unsucessfulLogin2" />
    <!--    <exclude name="unsucessfulLogin1" />   --> 
      </methods>
       </class>
    </classes>
  </test>
</suite>