<!-- Running a set of test cases together is call executing a Test Suite. -->
<!-- In TestNG framework, -->
<!-- we need to create testng.xml file to create and handle multiple test
classes. -->
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Test-Suite">
    <test name="sel-test-parameters">

        <parameter name="username" value="testuser_1" />
        <parameter name="password" value="Test@123" />

        <classes>

            <class name="testNG.parametersDemo" />
            
        </classes>

    </test>
</suite>